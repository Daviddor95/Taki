<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.game.taki.LeaderboardController">
   <children>
      <TableView fx:id="highscores" layoutX="200.0" layoutY="90.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="130.0" text="User name" />
          <TableColumn fx:id="scores" prefWidth="70.0" text="Wins" />
        </columns>
          <sortOrder>
              <fx:reference source="scores"/>
          </sortOrder>
      </TableView>
   </children>
</AnchorPane>
